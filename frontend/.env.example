# Frontend Environment Configuration - Development
# Copy this to .env.local for local development

# Enable dev-login for easy local testing
NEXT_PUBLIC_ENABLE_DEV_LOGIN=true

# Backend API Base URL (optional - defaults to /api/v1)
# Use relative path for same-origin (default), or full URL for external API
# NEXT_PUBLIC_API_BASE_URL=/api/v1
# NEXT_PUBLIC_API_BASE_URL=http://localhost:8000/api/v1
